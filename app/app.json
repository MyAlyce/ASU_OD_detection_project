{
	"configVersion": "v3",
	"app": {
		"appId": 22753,
		"appName": "MyAlyce",
		"appType": "app",
		"version": {
			"code": 1,
			"name": "1.0.0"
		},
		"icon": "icon.png",
		"vender": "zepp",
		"description": "MyAlyce app"
	},
	"permissions": [
		"device:os.bg_service",
		"data:user.hd.heart_rate",
		"data:user.hd.sleep",
		"data:user.info",
		"data:os.device.info",
		"device:os.notification",
		"device:os.local_storage"
	],
	"runtime": {
		"apiVersion": {
			"compatible": "3.0.0",
			"target": "3.0.0",
			"minVersion": "3.0"
		}
	},
	"targets": {
		"default": {
			"module": {
				"page": {
					"pages": [
						"page/index",
						"page/permissionsPage",
						"page/rescuePlanPage"
					]
				},
				"app-side": {
					"path": "app-side/index"
				},
				"app-service": {
					"services": ["app-service/service"]
				},
				"setting": {
					"path": "setting/index"
				}
			},
			"platforms": [
				{
					"st": "r",
					"dw": 480
				}
			]
		}
	},
	"i18n": {
		"en-US": {
			"appName": "MyAlyce"
		}
	},
	"defaultLanguage": "en-US"
}
